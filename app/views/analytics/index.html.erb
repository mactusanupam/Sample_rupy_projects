<p id="notice"><%= notice %></p>

<h1>Analytics</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>View count</th>
      <th>Vote count</th>
      <th>Ip address</th>
      <th>Session hash</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analytics.each do |analytic| %>
      <tr>
        <td><%= analytic.type %></td>
        <td><%= analytic.view_count %></td>
        <td><%= analytic.vote_count %></td>
        <td><%= analytic.ip_address %></td>
        <td><%= analytic.session_hash %></td>
        <td><%= link_to 'Show', analytic %></td>
        <td><%= link_to 'Edit', edit_analytic_path(analytic) %></td>
        <td><%= link_to 'Destroy', analytic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analytic', new_analytic_path %>

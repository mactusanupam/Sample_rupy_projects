<% if @question_upvote && @community_question.upvote_count.present? %>
	$("#upvote").addClass('disabled');
	$("#upvote").html("Liked | <b><%= @community_question.upvote_count %></b>");
<% end %>

<% if @answer_upvote && @community_answer.upvote_count.present? %>
	var id = "<%= @community_answer.id %>";

	$("#answers_upvote"+id).addClass('disabled');
	$("#answers_upvote"+id).html("Liked | <b><%= @community_answer.upvote_count %></b>");
	$("#arrow_answers_upvote"+id).html("<%= @community_answer.upvote_count %>");
<% end %>
